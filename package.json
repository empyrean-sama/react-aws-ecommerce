{
  "name": "react-aws-ecommerce",
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "version": "1.0.0",
  "scripts": {
    "start:frontend": "npx parcel src/index.html --dist-dir parcel-debug-cache",
    "build:frontend": "npx parcel build src/index.html --dist-dir public --no-source-maps",
