{
  "name": "react-aws-ecommerce",
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "version": "1.0.0",
  "scripts": {
    "start:frontend": "npx parcel src/index.html --dist-dir parcel-debug-cache",
    "build:frontend": "npx parcel build src/index.html --dist-dir public --no-source-maps",
    "deploy:backend": "cdk deploy --all --outputs-file outputs.json",
    "destroy:backend": "cdk destroy --all"
  },
  "devDependencies": {
    "@mui/types": "^7.4.8",
    "@parcel/transformer-typescript-tsc": "^2.16.1",
    "@types/react": "^19.2.6",
    "@types/react-dom": "^19.2.3",
    "aws-cdk": "^2.1033.0",
    "aws-cdk-lib": "^2.227.0",
    "buffer": "^6.0.3",
    "constructs": "^10.4.3",
    "esbuild": "^0.27.0",
    "parcel": "^2.16.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "uuid": "^13.0.0"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.5",
    "@mui/material": "^7.3.5",
    "aws-amplify": "^6.15.8",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "^7.9.6"
  }
}
